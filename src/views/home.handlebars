<h1>Home</h1>

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Titulo</th>
            <th>Descripcion</th>
            <th>Imagen</th>
            <th>Precio</th>
            <th>Stock</th>
        </tr>
        <tbody>
            {{#each products}}
                <tr>
                    <td>{{this.id}}</td>
                    <td>{{this.title}}</td>
                    <td>{{this.description}}</td>
                    <td><img src="{{this.image}}" alt="{{this.title}}" width="100"></td>
                    <td>{{this.price}}</td>
                    <td>{{this.stock}}</td>
                </tr>
            {{/each}}
        </tbody>
</table>